{
  "name": "task-mgt-nygroup-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node ./bin/www",
    "start:dev": "concurrently \"npx tsc --watch\" \"nodemon -q ./bin/www\"",
    "prettier": "prettier --write --ignore-unknown .",
    "test": "dotenv -e .env.test -- jest --detectOpenHandles --testTimeout=50000",
    "migrate:db-dev": "dotenv -e .env -- npx prisma migrate dev",
    "pretest": "dotenv -e .env.test -- npx prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "helmet": "^7.1.0",
    "http-errors": "^1.6.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "reflect-metadata": "^0.2.1",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@ebukaodini/scaffoldjs": "latest",
    "@faker-js/faker": "^8.4.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.8",
    "@types/supertest": "^6.0.2",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.4.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.11.0",
    "superagent": "^9.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
